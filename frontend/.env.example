# Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=60000

# App Configuration
REACT_APP_NAME=Resume Analyzer
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=AI-Powered Resume and Job Description Matching Tool

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_FEEDBACK=true
REACT_APP_ENABLE_DEBUG=false

# External Services (for future enhancements)
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn-here

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_FORMATS=PDF,DOCX,DOC,TXT