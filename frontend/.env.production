# Production Environment Variables

# API Configuration
REACT_APP_API_URL=/api
REACT_APP_API_TIMEOUT=60000

# App Configuration
REACT_APP_NAME=Resume Analyzer
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=AI-Powered Resume and Job Description Matching Tool

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_FEEDBACK=true
REACT_APP_ENABLE_DEBUG=false

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=4194304
REACT_APP_SUPPORTED_FORMATS=PDF,DOCX,DOC,TXT

# Build Configuration
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
CI=false